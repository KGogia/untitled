pipeline{
agent any
stages{

stage("build"){

steps{
sh "mvn -version"
sh "mvn clean install"
}
}

stage('reports') {
    steps {
    script {
            allure([
                    includeProperties: false,
                    jdk: '',
                    properties: [],
                    reportBuildPolicy: 'ALWAYS',
                    results: [[path: 'target/allure-results']]
            ])
    }
    }
}

}
tools {
    maven 'Maven3'
  }
}

post{
cleanWs()
}
